public class pattern5 {
    public static void main(String[] args)
    {
        for(int i=0;i<3;i++) {
            int k=i;
            for (int j = 1; j<=i+1; j++) {
                System.out.print((char)('A'+k));
                k++;
            }
            System.out.println();
        }
    }
}
