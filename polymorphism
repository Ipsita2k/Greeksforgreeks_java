class Bike
{
    void run(){
        System.out.println("drive");
    }
}
public class splendor extends Bike{
    void run(){
        System.out.println("safely");
    }
    public static void main(String[]args)
    {
        Bike b=new Bike();//b refence vriable for super class
        b.run();
        b=new splendor();
        b.run();
        //**super class variable can refer to sub class object**
    }
}
