import java.util.*;
public class stackintro {
    public static void main(String[]args){
        Stack<String> st = new Stack<>();
        st.push("a");
        st.push("b");
        st.push("c");
        st.push("d");
        System.out.println(st);
        st.pop();
        System.out.println(st);
        System.out.println(st.pop());
    }
}
